{
	"compilerOptions": {
		"target": "ESNext",
		"module": "NodeNext",
		"sourceMap": true,
		"outDir": "dist",
		"esModuleInterop": true,
		"strict": true,
		"noImplicitAny": true,
		"skipLibCheck": true,
		"moduleResolution": "NodeNext"
	},
	"include": ["src/**/*", "api/*.ts"],
	"exclude": ["node_modules"]
}
